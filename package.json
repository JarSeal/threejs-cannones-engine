{
  "name": "beaconjs",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "setup": "cd editor && yarn install && cd .. && yarn install && yarn envFiles",
    "dev": "yarn envFiles && concurrently --kill-others --names \"EDITOR\" -c \"bgBlue.bold\" \"cd editor && yarn dev\"",
    "lint:editor": "cd editor && yarn lint",
    "lint": "eslint .",
    "prettify": "prettier --write ./**/*.js",
    "envFiles": "node createEnvFiles.js",
    "test": "cd editor && yarn test",
    "build": "yarn install && cd editor && yarn install && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JarSeal"
  },
  "author": "JarSeal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JarSeal"
  },
  "homepage": "https://github.com/JarSeal",
  "dependencies": {
    "dotenv": "^16.0.1",
    "prettier": "^2.6.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "concurrently": "^7.2.0",
    "eslint": "^7.28.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0"
  },
  "engines": {
    "node": ">=16.13.2",
    "yarn": ">=1.22.15"
  }
}
